<%- include ./partials/head %>

<script>
    let navitem = document.getElementById('apps');
    navitem.classList.add("active");
    let mkAddNewApp = document.getElementById('mkAddNewApp');
    mkAddNewApp.classList.remove("hidden");

</script>
<br>
<div class="row">
    <div class="col-md-8 offset-md-2">

        <% if (apps !== undefined){ %>
        <% apps.forEach((app)=>{ %>

        <div class="card">
            <div class="card-body">
                <h5 class="card-title"><%= app.name %></h5>
                <p class="card-text"><%= app.packname%></p>

                <a href="/console/apps/<%= app.id %>" class="btn btn-primary">View Statics</a>

            </div>
        </div>
        <br>
        <% }) %>
        <% } %>

    </div>
</div>


<%- include ./partials/foot %>